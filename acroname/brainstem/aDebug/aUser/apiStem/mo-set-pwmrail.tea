
#include <aAPI.tea>
#include <aAPIStemDefs.tea>
#include <aMotionDefs.tea>

void main(int x)
{
  char id;
  char module;

  module = aAPI_GetObjByteVar(apiSTEM_MOTO_MODULE);
  id = aAPI_GetObjByteVar(apiSTEM_MOTO_CHANNEL);

  aAPI_SetTxByte0(cmdMO_CFG);
  aAPI_SetTxByteVar(1, id);
  aAPI_SetTxByte2(aMOTION_PARAM_PWMRAIL);
  aAPI_SetTxShortVar(3, x);
  aAPI_SetTxLength(5);

  aAPI_BeginTxVar(module);
}
