
#include <aAPI.tea>
#include <aAPIStemDefs.tea>
#include <aMotionDefs.tea>

void main()
{
  char id;
  char module;

  module = aAPI_GetObjByteVar(apiSTEM_MOTO_MODULE);
  id = aAPI_GetObjByteVar(apiSTEM_MOTO_CHANNEL);

  aAPI_SetTxByte0(cmdMO_SET);
  aAPI_SetTxByteVar(1, id);
  aAPI_SetTxLength(2);

  aAPI_SetRxFilterByte(cmdMO_SET);
  aAPI_SetRxTimeout(apiSTEM_DEFAULT_TIMEOUT);

  aAPI_BeginTxRxVar(module, module);

  // return value is in 0x0C23
}
