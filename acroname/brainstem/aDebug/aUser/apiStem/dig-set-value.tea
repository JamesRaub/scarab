
#include <aAPI.tea>
#include <aAPIStemDefs.tea>

void main(char val)
{
  char id;
  char module;

  module = aAPI_GetObjByteVar(apiSTEM_DIGITAL_MODULE);
  id = aAPI_GetObjByteVar(apiSTEM_DIGITAL_ID);

  aAPI_SetTxByte0(cmdDIG_IO);
  aAPI_SetTxByteVar(1, id);
  aAPI_SetTxByteVar(2, val);
  aAPI_SetTxLength(3);

  aAPI_BeginTxVar(module);
}
