
#include <aAPI.tea>
#include <aAPIStemDefs.tea>

void main()
{
  char addr;
  char module;

  module = aAPI_GetObjByteVar(apiSTEM_DIGITAL_MODULE);
  addr = aAPI_GetObjByteVar(apiSTEM_DIGITAL_ID);

  aAPI_SetTxByte0(cmdDIG_IO);
  aAPI_SetTxByteVar(1, (char)(128 + addr));
  aAPI_SetTxLength(2);

  aAPI_SetRxFilterByte(cmdDEV_VAL);
  aAPI_SetRxTimeout(apiSTEM_DEFAULT_TIMEOUT);

  aAPI_BeginTxVar(module);
  aAPI_BeginRxVar(module);

  // return value is in 0x0C22
}
